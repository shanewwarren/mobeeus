{
  "name": "slowmobius",
  "version": "1.0.0",
  "description": "Hapi.js plugin to handle queueing and scheduling of background jobs.",
  "repository": {
    "type": "git",
    "url": "git://github.com/shanewwarren/mobeeus"
  },
  "scripts": {
    "test": "mocha --recursive test",
    "one_way": "node ./examples/one_way/server & ./bin/mobeeus -f ./examples/one_way/worker.js -a 5000",
    "two_way": "node ./examples/two_way/server & ./bin/mobeeus -f ./examples/two_way/worker.js -a 500",
    "scheduling": "node ./examples/scheduling/server & ./bin/mobeeus -f ./examples/scheduling/worker.js -a 20000"
  },
  "author": "Shane Warren",
  "license": "BSD-3-Clause",
  "dependencies": {
    "agenda": "^0.9.0",
    "amqplib": "^0.4.1",
    "bossy": "^3.0.3",
    "items": "^2.1.1",
    "joi": "^10.0.5"
  },
  "devDependencies": {
    "eslint-config-hapi": "^10.0.0",
    "eslint-plugin-hapi": "^4.0.0",
    "hapi": "^16.0.1",
    "hoek": "^4.1.0",
    "chai": "^3.5.0",
    "mocha": "^3.2.0"
  },
  "keywords": [
    "hapijs",
    "rabbitmq",
    "agenda",
    "scheduler",
    "plugin"
  ]
}
